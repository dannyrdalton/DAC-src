<%= form_for @blogPost, url: { action: "create" }, html: { class: "blog-post-form" } do |f| %>
	<%= f.label :title %><br/>
	<%= f.text_field :title, :autofocus => true %><br/>
	<%= f.label :body %><br/>
	<%= f.text_area :body %><br/>
	<%= f.fields_for :tracks do |tracks_form| %>
		<%= tracks_form.label :link %><br/>
		<%= tracks_form.text_field :link %><br/>
	<% end %>
	<%= f.submit "Submit Post" %>
<% end %>
